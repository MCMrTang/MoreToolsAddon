<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="net.caborambo.addon.moretools"
        android:versionCode="1"
        android:versionName="1.0 beta 1">

    <uses-permission android:name="net.zhuoweizhang.mcpelauncher.ADDON"/>
    <uses-permission android:name="net.yosi.isageek.atomiclauncher.ADDON"/>
    
    <!-- This is the platform API where NativeActivity was introduced. -->
    <uses-sdk android:minSdkVersion="9" />

    <!-- This .apk has no Java code itself, so set hasCode to false. -->
    <application
		android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:hasCode="false"
        android:debuggable="false" >

    <!-- metadata specifying the library name. must match Android.mk.-->
    <meta-data
		android:name="net.zhuoweizhang.mcpelauncher.api.nativelibname"
        android:value="net.caborambo.addon.moretools" />
		<meta-data
			android:name="net.zhuoweizhang.mcpelauncher.api.targetmcpeversion"
			android:value="0.11.0" />
    </application>

</manifest> 
